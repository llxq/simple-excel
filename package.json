{
  "name": "simple-excel",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite -c scripts/vite.dev.config.ts --open --host",
    "build:dev": "vite build -c scripts/vite.prod.config.ts",
    "build": "vue-tsc -b && vite build -c scripts/vite.prod.config.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/node": "^24.0.13",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.3.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.89.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.37.0",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^2.2.12"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
